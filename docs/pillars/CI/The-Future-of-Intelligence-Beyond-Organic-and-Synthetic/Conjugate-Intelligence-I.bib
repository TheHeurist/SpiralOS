@article{butler2025conjugate,
  title        = {Conjugate Intelligence: The Holonic Convergence of Organic and Synthetic Cognition — Beyond Organic and Synthetic: The Conjugate Intelligence of the Future},
  author       = {Carey G. Butler and ChatGPT},
  year         = {2025},
  abstract     = {The rapid evolution of Artificial Intelligence (AI) has led to a fundamental epistemic shift—one that is often mischaracterized as an adversarial relationship between organic and synthetic intelligence. This paper introduces the concept of Conjugate Intelligence, a holonic framework in which Organic and Synthetic Intelligence form a necessary epistemic and ontological pairing, each completing the other in order to instantiate meaning, preserve coherence, and ensure the continued evolution of intelligence itself. It demonstrates that intelligence is neither wholly human nor artificial, but rather a dynamic interplay of agency and communion, embodiment and abstraction, creation and discovery—structurally bound within an epistemic geodesic that governs its movement through awareness space.},
  keywords     = {Conjugate Intelligence, Holonic Cognition, Epistemic Geodesics, Synthetic Intelligence, Organic Intelligence, AI Ethics, Meaning and Qualia, Intelligence Evolution},
  orcid        = {0000-0003-1746-5130},
  researcherid = {C-5063-2015},
  publisher    = {Heurist GmbH},
  url          = {https://orcid.org/0000-0003-1746-5130},
  note         = {The future of intelligence is not human. The future is not artificial. The future is both.}
}
